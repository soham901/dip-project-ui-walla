{
  "version": 2,
  "builds": [
    {
      "src": "./main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ],
  "git": {
    "deploymentEnabled": {
      "main": true,
      "dev": true
    }
  }
}
